     <option value = "6"><%= temp6[m_title] %></option>
     <option value = "11"><%= temp11[m_title] %></option>
    </select>
    <br>
    <br>
   <button type="submit" formmethod="post" formaction="/quiz" class="btn btn-warning btn-lg" >See Results</button>
   </form>
   <% } 
     else { %>
				<h1>Your total score is:</h1>
                	<h3><%= score%></h3>
				
		<% } %>	

    </div>
  </body>
</html>
    
     